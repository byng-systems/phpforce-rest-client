{
    "name": "phpforce/rest-client",
    "type": "library",
    "description": "A client for the Salesforce REST API",
    "keywords": [ "salesforce", "rest", "force.com", "web services" ],
    "license": "MIT",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "byng-systems/phpforce-soap-client",
                "source": {
                    "url": "https://github.com/byng-systems/phpforce-soap-client.git",
                    "type": "git",
                    "reference": "origin/master"
                },
                "autoload": {
                    "psr-0": {
                        "Phpforce\\SoapClient": "src/"
                    }
                }
            }
        }
    ],
    "require": {
        "guzzle/http": "*",
        "phpforce/soap-client": "*"
    },
    "autoload": {
        "psr-0": {
            "Phpforce\\RestClient": "src"
        }
    },
    "minimum-stability": "dev"
}
